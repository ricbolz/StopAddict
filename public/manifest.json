{
  "name": "StopAddict",
  "start_url": ".",
  "manifest_version": 3,
  "version" : "0.1",
  "description": "Stop Addict",
  "author" : "Hendrico Vernando Bolang",
  "permissions": [
    "storage",
    "tabs",
    "webRequest",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "declarativeNetRequestFeedback",
    "<all_urls>"
  ],
  "action" : {
    "default_popup" : "index.html",
    "default_title" : "StopAddict"
  },
  "default_icon": {
    "16" : "logo192.png",
    "32" : "logo192.png",
    "48" : "logo192.png",
    "128": "logo192.png"
  },
  "icons" : {
    "16" : "logo192.png",
    "32" : "logo192.png",
    "48" : "logo192.png",
    "128": "logo192.png"
  },
  "background": {
    "service_worker":"background.js"
  },
  "host_permissions" : [
    "*://example.com/*"
  ]

}
